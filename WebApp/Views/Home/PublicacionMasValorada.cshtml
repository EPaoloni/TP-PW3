
@{
    ViewBag.Title = "Ingresar";
    Layout = "~/Views/Home/_InicioLayout.cshtml";
}

<div class="row">
    <div class="col m12">
        <div>
            @Html.Partial("pv_Buscador")
        </div>
        <div class="row">
            <div class="titulo-inicio blue-text accent-3 center">
                <h5>PUBLICACIONES MAS VALORADAS</h5>
            </div>
            <hr />
        </div>
        <div class="row">
            @{
                foreach (Models.ORM.Necesidades necesidad in ViewBag.necesidades)
                {
                    @Html.Partial("pv_CardMasValorada", necesidad)
                }
            }
        </div>
    </div>
</div>